#include <bits/stdc++.h>
using namespace std;
int bi(int arr[] , int l , int r , int k){
    while(l <= r){
        int mid = (l + r)/2;
        if(arr[mid] == k) return mid;
        if(arr[mid] < k) l = mid+1;
        if(arr[mid] > k) r = mid - 1;
    }
    return -1;
}
int main(){
    return 0;
}
